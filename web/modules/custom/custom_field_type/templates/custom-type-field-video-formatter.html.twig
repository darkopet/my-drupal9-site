{#
/**
 * @file
 * Default theme implementation of a simple Youtube video.
 *
 * Available variables:
 *  - width: Youtube video width.
 *  - height: Youtube video height.
 *  - video_id: Youtube video ID.
 *
 *  @see template_preprocess()
 *  @see custom_field_type_video_formatter()
 *
 * @ingroup themeable
 */
#}
  {% spaceless %}
    <iframe width="{{ width }}" height="{{ height }}" src="https://www.youtube.com/embed/{{ video_id }}"
            allow="autoplay; encrypted-media" allowfullscreen></iframe>
  {% endspaceless %}
