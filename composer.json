{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "woocommerce/flexslider",
              "version": "2.0",
              "type": "drupal-library",
              "source": {
                "url": "https://github.com/woocommerce/FlexSlider.git",
                "type": "git",
                "reference": "2.7.2"
              }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "antennaio/jquery-bar-rating",
                "version": "1.2.2",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/antennaio/jquery-bar-rating/archive/v1.2.2.zip"
                }
            }
        }
    ],
    "drupal-scaffold": {
        "locations": {
            "web-root": "web/"
        },
        "file-mapping": {
            "[web-root]/sites/development.services.yml": false
        }
    },
    "require": {
        "alchemy/zippy": "^1.0",
        "antennaio/jquery-bar-rating": "^1.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.10",
        "drupal/avatar_field_formatter": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/core-composer-scaffold": "9.4.7",
        "drupal/core-project-message": "9.4.7",
        "drupal/core-recommended": "9.4.7",
        "drupal/devel": "^4.1",
        "drupal/devel_entity_updates": "^4.0",
        "drupal/development_environment": "^2.0",
        "drupal/drupal_slider": "^2.0",
        "drupal/email_validator": "^2.2",
        "drupal/entity_browser": "^2.8",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_reference_views": "^1.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/evac": "^1.0",
        "drupal/examples": "^3.0",
        "drupal/facets": "^2.0",
        "drupal/faq": "^1.1",
        "drupal/field_group": "^3.2",
        "drupal/field_slideshow": "^3.0@beta",
        "drupal/flag": "^4.0@beta",
        "drupal/flexslider": "^2.0",
        "drupal/fullcalendar": "^1.0@alpha",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/google_api_client": "^4.3",
        "drupal/google_calendar": "^2.4",
        "drupal/google_calendar_service": "^2.0@alpha",
        "drupal/group": "^1.5",
        "drupal/intense": "^2.3",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/jsonapi_extras": "^3.21",
        "drupal/key_auth": "^2.0",
        "drupal/libraries": "^3.0@beta",
        "drupal/mail_safety": "^1.2",
        "drupal/maillog": "^1.0@beta",
        "drupal/mailsystem": "^4.4",
        "drupal/message": "^1.2",
        "drupal/message_notify": "^1.2",
        "drupal/message_subscribe": "^1.1",
        "drupal/metatag": "^1.19",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/migrate_tools": "^6.0",
        "drupal/node_custom_status_messages": "^1.0",
        "drupal/openapi": "^2.0",
        "drupal/paragraphs": "^1.14",
        "drupal/paragraphs_ee": "^2.0",
        "drupal/paragraphs_features": "^1.16",
        "drupal/paragraphs_sets": "^2.8",
        "drupal/pathauto": "^1.10",
        "drupal/phone_international": "^3.2",
        "drupal/queue_throttle": "^1.2",
        "drupal/queue_ui": "^3.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/reroute_email": "^2.1",
        "drupal/restui": "^1.21",
        "drupal/schemata": "^1.0@beta",
        "drupal/search_api": "^1.25",
        "drupal/search_api_algolia": "^3.0@beta",
        "drupal/search_api_autocomplete": "^1.6",
        "drupal/search_api_opensearch": "^1.0@alpha",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/search_api_solr": "^4.2",
        "drupal/seo_checklist": "^5.1",
        "drupal/simple_oauth": "^5.2",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^3.5",
        "drupal/smtp": "^1.0",
        "drupal/social_auth": "^3.1",
        "drupal/social_auth_github": "^3.0",
        "drupal/social_auth_google": "^3.0",
        "drupal/starrating": "^4.1@alpha",
        "drupal/status_messages": "^4.3",
        "drupal/token": "^1.11",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/votingapi": "^3.0@beta",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/webform": "^6.1",
        "drupal/webform_submissions_delete": "^1.0",
        "drupal/xmlsitemap": "^1.2",
        "drupal/zooming": "^1.1",
        "drush/drush": "*",
        "google/apiclient": "^2.12",
        "league/oauth2-google": "^3.0",
        "platformsh/config-reader": "^2.4",
        "symfony/phpunit-bridge": "^6.1",
        "twig/twig": "2.15.3",
        "woocommerce/flexslider": "~2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "behat/behat": "^3.11",
        "dmore/behat-chrome-extension": "^1.4",
        "drupal/drupal-extension": "^4.2",
        "drush-ops/behat-drush-endpoint": "^9",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5"
    }
}
